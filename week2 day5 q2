import java.io.*;

import java.util.*;

public class Solution {

  public static void main(String[] args) {

    Scanner scan = new Scanner(System.in);

    double num = scan.nextDouble();

    if (num < 0) {

      System.out.println("Invalid Input");

    } else {

      int result = (int) num;

      System.out.println(result);

    }

  }

}
