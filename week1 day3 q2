import java.io.*;
import java.util.*;

public class Solution 
{

    public static void main(String[] args) 
    {
        int cube,N;
        Scanner input=new Scanner(System.in);
        N=input.nextInt();
        for (int i = 0; i <= N; i++)
        {
            cube = i * i * i;
            if (cube == N)
            {
                System.out.println("Perfect Cube");
                return;
            }
            else if (cube > N)
            {
                System.out.println("Not Perfect Cube");
                return;
            }
        }
    }
}
